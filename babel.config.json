{
	"presets": ["module:metro-react-native-babel-preset"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"extensions": [".ios.js", ".android.js", ".js", ".ts", ".tsx", ".json"],
				"alias": {
					"asmr": ["./src/"],
					"asmr-assets": ["./assets/"],
					"asmr-mobile": ["./"],
					"asmr-tests": ["./__tests__/"]
				}
			}
		],
		[
			"module:react-native-dotenv",
			{
				"safe": false
			}
		]
	]
}
